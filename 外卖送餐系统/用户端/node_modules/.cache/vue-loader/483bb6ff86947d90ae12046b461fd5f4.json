ick=\"lookImg(item.pimg)\" />\n        <span class=\"font12 mt10 font-grey\">{{ item.time }}</span>\n        <van-button color=\"rgb(102, 161, 136)\" size=\"mini\" class=\"p-a\" style=\"width: 50px; right: 0; bottom: 10px\" @click=\"take(item)\">代取</van-button>\n      </div>\n    </div>\n\n    <van-dialog v-model=\"show\" title=\"代取信息\" confirmButtonColor=\"rgb(102, 161, 136)\">\n      <div class=\"f-c p20\">\n        <span class=\"mb10\"\n          >核销码: <span class=\"font20\" style=\"color: rgb(102, 161, 136)\">{{ info.paycode }}</span></span\n        >\n        <span class=\"mb10\">联系人: {{ info.uname }}</span>\n        <span class=\"mb10\">联系电话: {{ info.tel }}</span>\n        <span>配送地址: {{ info.addr }}</span>\n      </div>\n    </van-dialog>\n  </div>\n</template>\n\n<script>\nimport * as api from \"@/api/api\";\nimport { ImagePreview } from \"vant\";\n\nexport default {\n  components: {},\n  data() {\n    return {\n      list: [],\n      show: false,\n      info: {}\n    };\n  },\n  created() {\n    if (!sessionStorage.getItem(\"id\")) {\n      this.$toast(\"请先登录\");\n      this.$router.push(\"/my\");\n      return;\n    }\n    this.getData();\n  },\n  mounted() {},\n  watch: {},\n  beforeDestroy() {},\n  computed: {},\n  methods: {\n    getData() {\n      api\n        .orderstakelist({\n          limit: 999,\n          offset: 1,\n          type: \"外卖配送\",\n          status: \"待核销\"\n        })\n        .then((res) => {\n          this.list = res.data.list;\n        });\n    },\n\n    lookImg(url) {\n      ImagePreview({\n        images: [url],\n        closeable: true\n      });\n    },\n\n    take(item) {\n      this.info = item;\n      this.show = true;\n    }\n  }\n};\n</script>\n\n<style lang=\"less\" scoped>\n.row-name {\n  color: #66789e;\n}\n.row-right {\n  border-bottom: 1px solid #ededed;\n}\n</style>\n"]}]}