\"login_foot\">\n      <p></p>\n      <p @click=\"$router.push('/register')\">注册</p>\n    </div>\n  </div>\n</template>\n\n<script>\nimport * as api from \"@/api/api\";\n\nexport default {\n  name: \"LoginIndex\",\n  data() {\n    return {\n      user: \"\",\n      pwd: \"\",\n      formRules: {\n        user: [\n          {\n            required: true,\n            message: \"请输入手机号\"\n          },\n          {\n            pattern: /^(?:(?:\\+|00)86)?1\\d{10}$/,\n            message: \"手机号格式错误\"\n          }\n        ],\n        pwd: [\n          {\n            required: true,\n            message: \"请输入密码\"\n          },\n          {\n            pattern: /^[A-Za-z0-9]{8,12}$/,\n            message: \"密码为8~12位数字字母组合\"\n          }\n        ]\n      }\n    };\n  },\n  computed: {},\n  watch: {},\n  methods: {\n    // 登录\n    login() {\n      api\n        .userlist({\n          limit: 1,\n          offset: 1,\n          user: this.user,\n          pwd: this.pwd\n        })\n        .then((res) => {\n          if (res.data.total === 0) {\n            this.$toast(\"账号密码错误!\");\n            return;\n          } else {\n            Object.keys(res.data.list[0]).forEach((k) => {\n              sessionStorage.setItem(k, res.data.list[0][k]);\n            });\n            this.$toast(\"登录成功!\");\n            this.$router.push(\"/\");\n          }\n        });\n    }\n  }\n};\n</script>\n\n<style scoped lang=\"less\">\n* {\n  margin: 0 auto;\n  padding: 0px;\n}\n.login_nav /deep/ .van-icon-arrow-left::before {\n  color: #fff;\n}\n.login_nav /deep/ .van-nav-bar__content {\n  background-color: rgb(102, 161, 136);\n}\n.login_nav /deep/ [class*=\"van-hairline\"]::after {\n  border: 0px;\n}\n.head {\n  width: 100%;\n  height: 220px;\n  background: rgb(102, 161, 136, 0.1);\n}\n.login_form {\n  padding-top: 20px;\n  // margin-top: 40px;\n  width: 300px;\n  // border: 1px solid #000;\n}\n.login_form /deep/ .van-cell {\n  margin: 45px 0;\n}\n.login_form /deep/ .icon-shouji::before {\n  color: rgb(102, 161, 136);\n  font-size: 25px;\n  font-weight: 600;\n  margin-right: 30px;\n}\n.login_form /deep/ .icon-mima54::before {\n  color: rgb(102, 161, 136);\n  font-size: 25px;\n  font-weight: 600;\n  margin-right: 30px;\n}\n.login_form /deep/ .icon-error::before {\n  color: rgb(102, 161, 136);\n  font-size: 20px;\n  font-weight: 600;\n  margin-right: 30px;\n}\n.login_form /deep/ .van-button::before {\n  height: 40px;\n  padding-top: 15px;\n  background-color: rgb(102, 161, 136);\n  border-color: rgb(102, 161, 136);\n  opacity: 1;\n  content: \"登录\";\n  font-size: 18px;\n}\n.login_foot {\n  display: flex;\n  justify-content: space-around;\n  margin-top: 30px;\n  color: #969799;\n}\n</style>\n"]}]}