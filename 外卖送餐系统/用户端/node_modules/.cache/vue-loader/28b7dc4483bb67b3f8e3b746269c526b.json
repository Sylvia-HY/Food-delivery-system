/template>\n\n<script>\nimport * as api from \"@/api/api\";\nimport BaiduMap from \"vue-baidu-map/components/map/Map.vue\";\nimport BmDriving from \"vue-baidu-map/components/search/Driving.vue\";\nimport { BmlLushu } from \"vue-baidu-map\";\n\nexport default {\n  components: { BaiduMap, BmDriving, BmlLushu },\n  data() {\n    return {\n      show: false,\n      center: { lng: 116.397128, lat: 39.916527 },\n      zoom: 10,\n      path: [],\n      icon: {\n        url: \"http://api.map.baidu.com/library/LuShu/1.2/examples/car.png\",\n        size: { width: 52, height: 26 },\n        opts: { anchor: { width: 27, height: 13 } },\n      },\n      start: \"\",\n      end: \"\",\n    };\n  },\n  created() {\n    this.getData();\n  },\n  mounted() {},\n  methods: {\n    // 获取位置\n    getData() {\n      api\n        .orderslist({\n          code: this.$route.query.code,\n          limit: 1,\n          offset: 1,\n        })\n        .then((res) => {\n          this.start = res.data.list[0].saddr;\n          this.end = res.data.list[0].addr;\n        });\n    },\n\n    handleSearchComplete(res) {\n      this.path = res.getPlan(0).getRoute(0).getPath();\n    },\n  },\n  computed: {},\n  watch: {},\n  beforeDestroy() {},\n};\n</script>\n\n<style lang='less' scoped>\n.commodity-head /deep/ .van-nav-bar__content {\n  background-color: rgb(102, 161, 136);\n  height: 70px;\n  line-height: 50px;\n}\n.commodity-head /deep/ .van-nav-bar__title {\n  font-size: 20px;\n  font-weight: 600;\n}\n.commodity-head /deep/ .van-icon-arrow-left::before {\n  font-size: 20px;\n  font-weight: 600;\n  //   margin-top: -160px;\n  color: #000;\n}\n\n.bm-view {\n  width: 100%;\n  height: calc(100vh - 70px);\n  // overflow: hidden;\n}\n</style>"]}]}