1EA,EA0EA;AAAA;;AACA,WAAA,OAAA,CAAA,SAAA,EAAA,IAAA,EAAA;AACA,QAAA,iBAAA,EAAA,IADA;AAEA,QAAA,gBAAA,EAAA;AAFA,OAAA,EAIA,IAJA,CAIA,gBAAA;AAAA,YAAA,KAAA,QAAA,KAAA;AACA,QAAA,GAAA,CACA,YADA,CACA;AACA,UAAA,IAAA,EAAA,IAAA,CAAA,KAAA,CAAA,CAAA,EAAA,IADA;AAEA,UAAA,UAAA,EAAA;AAFA,SADA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,cAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,YAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,YAAA,MAAA,CAAA,OAAA;AACA;AACA,SAVA;AAWA,OAhBA,EAiBA,KAjBA,CAiBA,YAAA,CAAA,CAjBA;AAkBA,KA7FA;AA+FA;AACA,IAAA,KAhGA,mBAgGA;AAAA;;AACA,WAAA,UAAA,GAAA,KAAA;AACA,UAAA,IAAA,GAAA,UAAA,CAAA,YAAA;AACA,QAAA,MAAA,CAAA,SAAA,CAAA,SAAA,EAAA,MAAA,EADA,CACA;;AACA,OAFA,EAEA,GAFA,CAAA;AAGA,KArGA;AAuGA;AACA,IAAA,GAxGA,eAwGA,IAxGA,EAwGA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,GAAA,CACA,SADA,CACA;AACA,kBAAA,IAAA,EAAA,IAAA,CAAA;AADA,iBADA,EAIA,IAJA,CAIA,UAAA,GAAA,EAAA;AACA,sBAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;AACA,mBAFA,MAEA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,iBAVA,CADA;;AAAA;AAAA;AAAA,uBAaA,MAAA,CAAA,OAAA,EAbA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAcA,KAtHA;AAwHA;AACA,IAAA,OAzHA,qBAyHA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAEA,gBAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,OAAA,EAAA,GAAA,CAAA,UAAA,CAAA,EAAA;AACA,sBAAA,CAAA,MAAA,CAAA,OAAA,CAAA,CAAA,CAAA,EAAA;AACA,0BAAA,IAAA,KAAA,CAAA,CAAA,CAAA;AACA;AACA,iBAJA;AAFA;AAAA;;AAAA;AAAA;AAAA;;AAQA,gBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,SAAA;;AARA;;AAAA;AAAA;AAAA,uBAYA,GAAA,CAAA,UAAA,CAAA,MAAA,CAAA,OAAA,EAAA,IAAA,CAAA,UAAA,GAAA,EAAA;AACA,sBAAA,GAAA,CAAA,IAAA,IAAA,GAAA,EAAA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,OAAA,CAAA,OAAA;;AACA,oBAAA,MAAA,CAAA,UAAA,GAAA,KAAA;AACA,mBAHA,MAGA;AACA,oBAAA,MAAA,CAAA,QAAA,CAAA,KAAA,CAAA,GAAA,CAAA,OAAA;AACA;AACA,iBAPA,CAZA;;AAAA;AAAA;AAAA,uBAoBA,MAAA,CAAA,OAAA,EApBA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAqBA,KA9IA;AAgJA;AACA,IAAA,IAjJA,gBAiJA,IAjJA,EAiJA;AACA,WAAA,OAAA,GAAA,IAAA,CAAA,KAAA,CAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,CAAA,CADA,CACA;;AACA,WAAA,UAAA,GAAA,IAAA;AACA;AApJA,GAtBA;AA4KA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AAAA;;AACA,aAAA,UAAA,IAAA,EAAA;AACA,YAAA,KAAA,GAAA,CAAA;AACA,QAAA,IAAA,CAAA,OAAA,CAAA,UAAA,CAAA,EAAA;AACA,UAAA,KAAA,IAAA,MAAA,CAAA,SAAA,CAAA,KAAA,CAAA,MAAA,CAAA,CAAA,CAAA,GAAA,CAAA,EAAA,MAAA,CAAA,CAAA,CAAA,KAAA,CAAA,CAAA,CAAA;AACA,SAFA;AAGA,eAAA,MAAA,CAAA,KAAA,CAAA,KAAA,CAAA,KAAA,EAAA,CAAA,CAAA;AACA,OANA;AAOA;AATA;AA5KA,CAAA","sourcesContent":["<template>\n  <div class=\"dashboard-container\">\n    <el-form ref=\"searchForm\" :model=\"searchForm\" inline size=\"medium\" class=\"searchForm\" label-width=\"80px\">\n      <el-form-item label=\"订单号:\">\n        <el-input v-model=\"searchForm.code\" placeholder=\"\" style=\"width: 250px\"></el-input>\n      </el-form-item>\n      <el-form-item>\n        <el-button type=\"primary\" @click=\"\n            searchForm.code = '';\n            getData();\n          \">重置</el-button>\n        <el-button type=\"primary\" @click=\"getData()\" icon=\"el-icon-search\">查询</el-button>\n        <!-- <el-button @click=\"showDialog = true\" icon=\"el-icon-plus\" plain>新增</el-button> -->\n      </el-form-item>\n    </el-form>\n\n    <el-card slot class=\"content-card\" shadow=\"never\">\n      <el-table :data=\"list\" border stripe style=\"width: 100%; height: 60vh;overflow-y:scroll\">\n        <el-table-column type=\"expand\">\n          <template slot-scope=\"props\">\n            <div class=\"f-c mb30\" v-if=\"props.row.type == '外卖配送'\">\n              <span class=\"mb10\">联系电话: {{ props.row.goods[0].tel }}</span>\n              <span>配送地址: {{ props.row.goods[0].addr }}</span>\n            </div>\n            <div>\n              <div class=\"f-r f-a-c f-j-s\" v-for=\"item in props.row.goods\" :key=\"item.id\">\n                <div class=\"f-r f-a-c mb10\">\n                  <img :src=\"item.img\" alt=\"\" style=\"width: 60px; height: 60px\" />\n                  <span class=\"ml20\">{{ item.name }}</span>\n                </div>\n                <div>\n                  <span>{{ item.price }}</span>\n                  x\n                  <span>{{ item.num }}</span>\n                </div>\n              </div>\n              <el-divider></el-divider>\n              <div class=\"t-r\">合计: {{ sumPrice(props.row.goods) }}</div>\n              <div class=\"mb10 font-yellow\">用户评价:{{props.row.reply}}</div>\n              <div class=\"pb10 font-yellow\">商家回复:{{props.row.storereply}}</div>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column label=\"序号\" header-align=\"center\" align=\"center\" type=\"index\" width=\"70\" />\n        <el-table-column prop=\"code\" label=\"订单号\" header-align=\"center\" align=\"center\" width=\"200\" />\n        <el-table-column prop=\"paycode\" label=\"核销码\" header-align=\"center\" align=\"center\" />\n        <el-table-column prop=\"sname\" label=\"下单店铺\" header-align=\"center\" align=\"center\" />\n        <el-table-column prop=\"img\" label=\"用户头像\" header-align=\"center\" align=\"center\">\n          <template slot-scope=\"scope\">\n            <img :src=\"scope.row.uimg\" style=\"width: 80px; height: 80px; border-radius: 50%\" alt=\"\" />\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"uname\" label=\"下单用户\" header-align=\"center\" align=\"center\" />\n        <el-table-column prop=\"type\" label=\"订单类型\" header-align=\"center\" align=\"center\" />\n        <el-table-column prop=\"status\" label=\"订单状态\" header-align=\"center\" align=\"center\" />\n        <el-table-column prop=\"time\" label=\"时间\" header-align=\"center\" align=\"center\" width=\"180\" />\n        <el-table-column prop=\"address\" label=\"操作\" header-align=\"center\" align=\"center\" width=\"190px\">\n          <template slot-scope=\"scope\">\n            <div class=\"f-r f-a-c f-j-c\" v-show=\"sid\">\n              <a style=\"color: rgb(31, 144, 254); margin-right: 20px\" @click=\"verification(scope.row)\" v-if=\"scope.row.status == '待核销' && scope.row.type == '店内就餐'\">核销</a>\n              <a style=\"color: rgb(31, 144, 254); margin-right: 20px\" @click=\"look(scope.row)\" v-if=\"scope.row.goods[0].reply\">查看反馈</a>\n              <a style=\"color: rgb(31, 144, 254); margin-right: 20px\" @click=\"storereply(scope.row)\" v-if=\"scope.row.goods[0].reply && !scope.row.goods[0].storereply\">回复</a>\n              <a style=\"color: rgb(31, 144, 254)\" @click=\"del(scope.row)\">删除</a>\n            </div>\n          </template>\n        </el-table-column>\n      </el-table>\n      <!-- 分页 -->\n      <!-- <Pagination :limit.sync=\"limit\" :total=\"total\" :offset.sync=\"offset\" :hidden=\"hidden\" @changePage=\"getData\" /> -->\n    </el-card>\n  </div>\n</template>\n\n<script>\nimport Middle from \"@/components/Middle/index.vue\";\nimport Dialog from \"@/components/Dialog/index.vue\";\nimport Pagination from \"@/components/Pagination/index.vue\";\nimport Enduit from \"@/components/WangEnduit/index.vue\";\nimport * as api from \"@/api/api\";\nimport calculate from \"@/utils/calculate\";\n\nexport default {\n  components: { Middle, Dialog, Pagination, Enduit },\n  data() {\n    return {\n      url: process.env.VUE_APP_BASE_FILE,\n      offset: 1, //当前页\n      limit: 10, //每页大小\n      total: 0, //总记录数\n      hidden: false, //分页是否隐藏\n      searchForm: {\n        code: \"\",\n      },\n      list: [],\n      showDialog: false,\n      sid: \"\",\n    };\n  },\n  created() {\n    this.sid = sessionStorage.getItem(\"id\");\n    this.getData();\n  },\n  mounted() {},\n  methods: {\n    //获取列表\n    getData() {\n      api\n        .orderslist({\n          code: this.searchForm.code,\n          offset: 1,\n          limit: 999,\n          sid: sessionStorage.getItem(\"id\") || \"\",\n        })\n        .then((res) => {\n          this.total = res.data.total;\n          if (res.code == 200) {\n            let list = [];\n            if (res.data.total > 0) {\n              let arr = {};\n              res.data.list.forEach((i) => {\n                if (!arr[i.code]) {\n                  arr[i.code] = {\n                    id: i.id,\n                    code: i.code,\n                    sname: i.sname,\n                    uname: i.uname,\n                    uimg: i.uimg,\n                    simg: i.simg,\n                    sid: i.sid,\n                    time: i.time,\n                    goods: [i],\n                    status: i.status,\n                    type: i.type,\n                    paycode: i.paycode,\n                    reply: i.reply,\n                    storereply: i.storereply,\n                  };\n                } else {\n                  arr[i.code].goods.push(i);\n                }\n              });\n              Object.values(arr).forEach((i) => {\n                list.push(i);\n              });\n\n              let sortArr = list.sort((a, b) => {\n                return b.id - a.id;\n              });\n              list = sortArr;\n            }\n            this.list = list;\n          }\n        });\n    },\n\n    // 核销\n    verification(item) {\n      api\n        .changestatus({\n          code: item.code,\n          status: \"已核销\",\n        })\n        .then((res) => {\n          if (res.code == 200) {\n            this.$message.success(\"核销成功!\");\n            this.getData();\n          }\n        });\n    },\n\n    look(item) {\n      this.$alert(item.goods[0].reply, \"反馈\", {\n        confirmButtonText: \"确定\",\n        callback: (action) => {},\n      });\n    },\n\n    storereply(item) {\n      this.$prompt(\"请输入回复内容\", \"提示\", {\n        confirmButtonText: \"确定\",\n        cancelButtonText: \"取消\",\n      })\n        .then(({ value }) => {\n          api\n            .changestatus({\n              code: item.goods[0].code,\n              storereply: value,\n            })\n            .then((res) => {\n              if (res.code == 200) {\n                this.$message.success(\"回复成功!\");\n                this.getData();\n              }\n            });\n        })\n        .catch(() => {});\n    },\n\n    //模态框-关闭\n    close() {\n      this.showDialog = false;\n      let time = setTimeout(() => {\n        this.resetData(\"addForm\", this); //重置数据\n      }, 300);\n    },\n\n    //删除\n    async del(item) {\n      await api\n        .ordersdel({\n          code: item.code,\n        })\n        .then((res) => {\n          if (res.code == 200) {\n            this.$message.success(\"操作成功!\");\n          } else {\n            this.$message.error(res.message);\n          }\n        });\n\n      await this.getData();\n    },\n\n    // 新增/编辑-提交\n    async confirm() {\n      try {\n        Object.keys(this.addForm).map((k) => {\n          if (!this.addForm[k]) {\n            throw new Error(k);\n          }\n        });\n      } catch (error) {\n        this.$message.error(\"请填写所有信息\");\n        return;\n      }\n\n      await api.orderssave(this.addForm).then((res) => {\n        if (res.code == 200) {\n          this.$message.success(\"操作成功!\");\n          this.showDialog = false;\n        } else {\n          this.$message.error(res.message);\n        }\n      });\n      await this.getData();\n    },\n\n    //编辑\n    edit(item) {\n      this.addForm = JSON.parse(JSON.stringify(item)); //对象拷贝防止重置对象时污染\n      this.showDialog = true;\n    },\n  },\n  computed: {\n    sumPrice() {\n      return (rows) => {\n        let total = 0;\n        rows.forEach((i) => {\n          total += Number(calculate.cheng(Number(i.num), Number(i.price)));\n        });\n        return this.$tool.round(total, 2);\n      };\n    },\n  },\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.dashboard {\n  &-container {\n    margin: 30px;\n  }\n  &-text {\n    font-size: 30px;\n    line-height: 46px;\n  }\n}\n>>> .avatar-uploader .el-upload {\n  border: 1px dashed #d9d9d9;\n  border-radius: 6px;\n  cursor: pointer;\n  position: relative;\n  overflow: hidden;\n}\n.avatar-uploader .el-upload:hover {\n  border-color: #409eff;\n}\n.avatar-uploader-icon {\n  border: 1px #c7c7c7 dashed;\n  border-radius: 10px;\n  font-size: 28px;\n  color: #8c939d;\n  width: 178px;\n  height: 178px;\n  line-height: 178px;\n  text-align: center;\n}\n.avatar {\n  width: 178px;\n  height: 178px;\n  display: block;\n}\n</style>\n"],"sourceRoot":"src/views/orders"}]}